<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    tools:context=".MainActivity">
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/bottom_imageView1"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@drawable/btn_yellow"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@id/imageView3"
        app:layout_constraintBottom_toBottomOf="parent" />



    <ImageView
        android:id="@+id/graph_background_imageview1"
        android:layout_width="match_parent"
        android:layout_marginRight="33dp"
        android:layout_marginLeft="33dp"
        android:layout_height="300dp"
        android:background="@drawable/background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.178" />
    <TextView
        android:id="@+id/graph_textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="그래프 시작 날짜"
        android:textSize="25sp"
        app:layout_constraintTop_toBottomOf="@id/graph_background_imageview1"
        app:layout_constraintLeft_toRightOf="@id/graph_imageview1"
        app:layout_constraintHorizontal_bias="0.0"
        />
    <ImageView
        android:id="@+id/graph_imageview1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:srcCompat="@drawable/baseline_calendar_today_black_18dp"
        app:layout_constraintLeft_toLeftOf="@id/graph_background_imageview1"
        app:layout_constraintRight_toLeftOf="@id/graph_textView1"
        app:layout_constraintTop_toTopOf="@id/graph_textView1"
        />
    <ImageView
        android:id="@+id/graph_imageview2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="5dp"
        app:srcCompat="@drawable/baseline_calendar_today_black_18dp"
        app:layout_constraintLeft_toLeftOf="@id/graph_background_imageview1"
        app:layout_constraintRight_toLeftOf="@id/graph_textView1"
        app:layout_constraintTop_toBottomOf="@id/graph_textView1"
        />
    <TextView
        android:id="@+id/graph_textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="그래프 종료 날짜"
        android:textSize="25sp"
        app:layout_constraintTop_toBottomOf="@id/graph_textView1"
        app:layout_constraintLeft_toRightOf="@id/graph_imageview2"
        app:layout_constraintHorizontal_bias="0.0"
        />
    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/food"
        app:layout_constraintStart_toStartOf="@id/textView8"
        app:layout_constraintEnd_toEndOf="@id/textView8"
        app:layout_constraintBottom_toTopOf="@id/textView8"
        />
    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="식단/체중"
        android:textSize="20sp"
        android:layout_marginBottom="10dp"
        app:layout_constraintRight_toLeftOf="@id/textView9"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/bottom_imageView1"
        app:layout_constraintHorizontal_bias="0.0"
        />
    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="98dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/baseline_show_chart_black_36dp"
        app:layout_constraintEnd_toEndOf="@id/textView9"
        app:layout_constraintStart_toStartOf="@id/textView9"
        app:layout_constraintLeft_toRightOf="@id/imageView3"
        app:layout_constraintBottom_toTopOf="@id/textView9"
        />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:text="체중 그래프"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@id/textView8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/textView8"
        app:layout_constraintRight_toLeftOf="@id/textView10"
        />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="20dp"
        android:text="메인화면"
        android:textSize="20sp"

        app:layout_constraintLeft_toRightOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/textView8"
        app:layout_constraintHorizontal_bias="0.0"
        />
    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/hiclipart2"
        app:layout_constraintEnd_toEndOf="@id/textView10"
        app:layout_constraintStart_toStartOf="@id/textView10"
        app:layout_constraintLeft_toRightOf="@id/imageView3"
        app:layout_constraintBottom_toTopOf="@id/textView9"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>