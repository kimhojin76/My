<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    tools:context=".MainActivity">
    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/forum_board"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mc_button8"

            />
        <ImageView
            android:id="@+id/forum_add"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginTop="25dp"
            app:srcCompat="@drawable/plus2"
            app:layout_constraintLeft_toLeftOf="@id/imageView7"
            app:layout_constraintBottom_toBottomOf="@+id/forum_board" />
        <ImageView
            android:id="@+id/bottom_imageView1"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="@drawable/btn_yellow"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@id/imageView3"
            app:layout_constraintBottom_toBottomOf="parent" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:layout_marginTop="30dp"
            app:srcCompat="@drawable/food"
            app:layout_constraintStart_toStartOf="@id/textView8"
            app:layout_constraintEnd_toEndOf="@id/textView8"
            app:layout_constraintBottom_toTopOf="@id/textView8"
            />
        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="식단"
            android:textSize="17dp"
            app:layout_constraintRight_toLeftOf="@id/textView9"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="@id/bottom_imageView1"
            />
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:layout_marginTop="30dp"
            app:srcCompat="@drawable/baseline_show_chart_black_36dp"
            app:layout_constraintEnd_toEndOf="@id/textView9"
            app:layout_constraintStart_toStartOf="@id/textView9"
            app:layout_constraintLeft_toRightOf="@id/imageView3"
            app:layout_constraintBottom_toTopOf="@id/textView9"
            />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0dp"
            android:text="그래프"
            android:textSize="17dp"
            app:layout_constraintBottom_toBottomOf="@id/textView8"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/textView8"
            app:layout_constraintRight_toLeftOf="@id/textView10"
            />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="메인"
            android:textSize="17dp"
            app:layout_constraintBottom_toBottomOf="@id/textView8"
            app:layout_constraintLeft_toRightOf="@+id/textView9"
            app:layout_constraintRight_toLeftOf="@id/textView5" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:layout_marginTop="30dp"
            app:srcCompat="@drawable/hiclipart2"

            app:layout_constraintEnd_toEndOf="@id/textView10"
            app:layout_constraintStart_toStartOf="@id/textView10"
            app:layout_constraintLeft_toRightOf="@id/imageView3"
            app:layout_constraintBottom_toTopOf="@id/textView9"
            />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="40dp"
            android:layout_height="35dp"
            app:layout_constraintBottom_toTopOf="@id/textView9"

            app:layout_constraintEnd_toEndOf="@+id/textView5"
            app:layout_constraintLeft_toRightOf="@id/imageView3"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:srcCompat="@drawable/calculator" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="40dp"
            android:layout_height="35dp"
            app:layout_constraintBottom_toTopOf="@id/textView9"

            app:layout_constraintEnd_toEndOf="@+id/textView16"
            app:layout_constraintLeft_toRightOf="@id/imageView3"
            app:layout_constraintStart_toStartOf="@+id/textView16"
            app:srcCompat="@drawable/chat" />
        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="칼로리"
            android:textSize="17dp"
            app:layout_constraintLeft_toRightOf="@id/textView10"
            app:layout_constraintRight_toLeftOf="@id/textView16"
            app:layout_constraintTop_toTopOf="@+id/textView10" />


        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="게시판"
            android:textSize="17dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="@+id/textView10" />







    </androidx.constraintlayout.widget.ConstraintLayout>




</ScrollView>